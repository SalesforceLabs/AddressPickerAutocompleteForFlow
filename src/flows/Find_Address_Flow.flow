<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignments>
        <name>Set_New_Lng_Lat</name>
        <label>Set New Lng &amp; Lat</label>
        <locationX>304</locationX>
        <locationY>158</locationY>
        <assignmentItems>
            <assignToReference>Longitude</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>addressPickerCmp.longitude</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>Latitude</assignToReference>
